fn add(x : int, y : int) : int {
  ret x + y;
}

fn sub(x : int, y : int) : int {
  ret x - y;
}

fn mul(x : int, y : int) : int {
  ret x * y;
}

fn div(x : int, y : int) : int {
  ret x / y;
}

fn main() {
  lem a : int = 5;
  mlem b : int = 6, c : int = 10;
  mlet x : int = 4, y : int = 2;
  let mut res : int = 0;
  for i in 0 to 10 {
    if i < 5 {
      res = call add(x, y);
    } else {
      res = call sub(x, y);
    }
  }
}
