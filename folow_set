SIGUIENTES(<cparenthesis>)  =  { ;, ε, : }
SIGUIENTES(<obracket>)  =  { ε, identifier, if, while, call, for, let, lem, ret }
SIGUIENTES(<cbracket>)  =  { ε, identifier, if, while, call, for, }, else, fn }
SIGUIENTES(<colon>)  =  { int, float, char, str, bool }
SIGUIENTES(<semicolon>)  =  { }, ε, identifier, if, while, call, for }
